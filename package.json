{
  "name": "ele-search-engine",
  "version": "0.0.1",
  "description": "Angular 6 with Electron",
  "author": {
    "name": "Zongqi Wei",
    "email": "weizongqi926@gmail.com"
  },
  "main": "./index.js",
  "scripts": {
    "ng": "ng",
    "build": "npm run postinstall:electron && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall:electron": "node postinstall",
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "build:prod": "npm run build -- -c production",
    "electron:local": "npm run build:prod && electron .",
    "start": "yarn electron:local",
    "dev": "concurrently \"ng serve\" \"electron . --serve\"",
    "mac": "npm run build:prod && npx electron-builder build --mac",
    "mac:start": "npm run build:prod && npx electron-builder start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.2",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "electron-json-storage": "^4.1.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.0",
    "os": "^0.1.1",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.11",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "electron": "^2.0.5",
    "electron-builder": "^20.26.1",
    "electron-reload": "^1.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npx": "^10.2.0",
    "protractor": "~5.3.0",
